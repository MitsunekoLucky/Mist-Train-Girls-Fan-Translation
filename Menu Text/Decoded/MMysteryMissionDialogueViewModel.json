[
    {
        "MMysteryMissionGroupId": 1,
        "Characters": [
            {
                "MCharacterId": 29201,
                "Phrases": [
                    {
                        "MMissionId": null,
                        "PhraseType": 0,
                        "Phrase": "{\"EN\":\"Conductor, the groove in this wall seems mighty suspicious! Let's check it out!\", \"TW\":\"列車長先生，牆壁上的凹痕感覺很可疑呢！我們調查看看吧！\", \"CN\":\"列车长先生，墙壁上的凹痕感觉很可疑呢！我们调查看看吧！\"}",
                        "StartDate": null,
                        "EndDate": null
                    },
                    {
                        "MMissionId": 813000001,
                        "PhraseType": 1,
                        "Phrase": "{\"EN\":\"Those Mist Monsters that look like dogs must be what we're after. Let's take 'em down!\", \"TW\":\"帶著項圈的小型野獸，也就是那隻像狗一樣的迷霧怪吧？我們快點打倒牠吧！\", \"CN\":\"带着项圈的小型野兽，也就是那只像狗一样的迷雾怪吧？我们快点打倒牠吧！\"}",
                        "StartDate": "2021-09-16T05:00:00Z",
                        "EndDate": null
                    },
                    {
                        "MMissionId": 813000002,
                        "PhraseType": 1,
                        "Phrase": "{\"EN\":\"Charge your energy… It must mean that stuff we’re always drinking! Please be sure not to overdo it though!\", \"TW\":\"補充能量……一定是指平常在喝的那個吧！小心不要喝太多了哦！\", \"CN\":\"补充能量……一定是指平常在喝的那个吧！小心不要喝太多了哦！\"}",
                        "StartDate": "2021-09-17T05:00:00Z",
                        "EndDate": null
                    },
                    {
                        "MMissionId": 813000003,
                        "PhraseType": 1,
                        "Phrase": "{\"EN\":\"Free Stuff? You are making sure you check every day, right sir?\", \"TW\":\"免費的服務？列車長先生每天都有記得抽嗎？\", \"CN\":\"免费的服务？列车长先生每天都有记得抽吗？\"}",
                        "StartDate": "2021-09-18T05:00:00Z",
                        "EndDate": null
                    },
                    {
                        "MMissionId": 813000004,
                        "PhraseType": 1,
                        "Phrase": "{\"EN\":\"I hear there's all kinds of scary demons in St. Iris. Huh, who did I hear that from? It must have been Putra…\", \"TW\":\"在聖伊麗絲似乎有非常可怕的鬼呢。咦？你問我是聽誰說的？我記得是布特拉跟我說的……\", \"CN\":\"在圣伊丽丝似乎有非常可怕的鬼呢。咦？你问我是听谁说的？我记得是布特拉跟我说的……\"}",
                        "StartDate": "2021-09-19T05:00:00Z",
                        "EndDate": null
                    },
                    {
                        "MMissionId": 813000005,
                        "PhraseType": 1,
                        "Phrase": "{\"EN\":\"A witch? I do remember hearing about a witch in the forest of St. Iris…\", \"TW\":\"魔女？這麼說來，在聖伊麗絲的森林裡出現了魔女……\", \"CN\":\"魔女？这么说来，在圣伊丽丝的森林里出现了魔女……\"}",
                        "StartDate": "2021-09-20T05:00:00Z",
                        "EndDate": null
                    },
                    {
                        "MMissionId": 813000006,
                        "PhraseType": 1,
                        "Phrase": "{\"EN\":\"Birds of prey? We had one of those in the SSS, yeah! Wait? No? I'm thinking of something else?\", \"TW\":\"猛禽類？在特鐵隊裡面也有呢！咦？不對嗎？不是自〇而是自我？\", \"CN\":\"猛禽类？在特铁队里面也有呢！咦？不对吗？不是自〇而是自我？\"}",
                        "StartDate": "2021-09-21T05:00:00Z",
                        "EndDate": null
                    },
                    {
                        "MMissionId": null,
                        "PhraseType": 2,
                        "Phrase": "{\"EN\":\"Something must be waiting for us on the other side of these walls. Sir, let's try and get it open!\", \"TW\":\"不知道牆壁的裡面有什麼在等著我們。列車長先生，我們趕快打開來看看吧！\", \"CN\":\"不知道墙壁的里面有什么在等着我们。列车长先生，我们赶快打开来看看吧！\"}",
                        "StartDate": "2021-09-21T05:00:00Z",
                        "EndDate": null
                    },
                    {
                        "MMissionId": null,
                        "PhraseType": 3,
                        "Phrase": "{\"EN\":\"That sure was fun!♪ Let's go look for some other ruins!\", \"TW\":\"真開心呢♪　我們一起去尋找新的遺跡吧！\", \"CN\":\"真开心呢♪　我们一起去寻找新的遗迹吧！\"}",
                        "StartDate": "2021-09-21T05:00:00Z",
                        "EndDate": null
                    },
                    {
                        "MMissionId": null,
                        "PhraseType": 0,
                        "Phrase": "{\"EN\":\"Conductor, I think this door might open if we fill in all the grooves!\", \"TW\":\"列車長先生，看來只要將凹陷的地方全都填上，就有可能打開這扇門了！\", \"CN\":\"列车长先生，看来只要将凹陷的地方全都填上，就有可能打开这扇门了！\"}",
                        "StartDate": null,
                        "EndDate": null
                    }
                ]
            }
        ]
    },
