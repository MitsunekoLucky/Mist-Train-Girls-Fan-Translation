},
{
    "GroupOrder": 20,
    "ViewOrder": 1,
    "Name": "ブランシュ",
    "Phrase": "「むしろ、そういう恋愛感情とかよくわかんないし――\\n　毎年バレンタインは浮かれた空気に馴染めなくてね、\\n　身の置き場がなくて困ったりしてたんだ」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": 57201,
    "IsSpeakingFlags": 0,
    "FrameImage": 0,
