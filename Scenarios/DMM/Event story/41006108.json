        "CameraRotation": null
    }
},
{
    "GroupOrder": 16,
    "ViewOrder": 1,
    "Name": "テルミニ",
    "Phrase": "「わたしには、これ以上は何もできないってこと。\\n　でも、この場にはわたし以外にも――あなたがいる。\\n　人を愛し、街を愛し、すべてを愛するわたしの生徒が」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": null,
    "IsSpeakingFlags": 0,
    "FrameImage": 0,
    "VoiceFileName": "e_42005208_010",
