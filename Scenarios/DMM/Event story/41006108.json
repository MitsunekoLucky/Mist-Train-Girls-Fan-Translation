    "GroupOrder": 18,
    "ViewOrder": 1,
    "Name": "パッスィ",
    "Phrase": "「……はい。\\n　ご期待ありがとうございます、テルミニ先生」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": null,
