        "CameraWorkDuration": null,
        "CameraRotation": null
    }
},
{
    "GroupOrder": 15,
    "ViewOrder": 1,
    "Name": "フェルクリンゲン",
    "Phrase": "「ひれ伏せっ、愚民ども！　称えろっ、あたしを！\\n　よくもよくも、これまでさんざん煮え湯を飲ませてくれやがったわねぇ！\\n　たっぷり溜まったこの怨み、百万倍にして返してやるからっ！」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": 5201,
    "IsSpeakingFlags": 0,
    "FrameImage": 0,
    "VoiceFileName": "e_42001205_010",
