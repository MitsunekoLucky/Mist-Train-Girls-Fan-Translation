},
{
    "GroupOrder": 18,
    "ViewOrder": 1,
    "Name": "シブヤ",
    "Phrase": "「ういっす！　さぁ、悪い子はお尻ペンペンしてやりますよ！」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
