        "CameraWorkDuration": null,
        "CameraRotation": null
    }
},
{
    "GroupOrder": 16,
    "ViewOrder": 1,
    "Name": "フュッセン",
    "Phrase": "「次、同じような侮辱をすれば貴様の首を刎ねる」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": 17201,
    "IsSpeakingFlags": 0,
    "FrameImage": 0,
