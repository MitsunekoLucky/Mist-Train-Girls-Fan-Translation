},
{
    "GroupOrder": 11,
    "ViewOrder": 1,
    "Name": "カールスルーエ",
    "Phrase": "「……あなたには何も言っていないんですけどねぇ、\\n　『フュッセンお嬢ちゃま』？」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": 19201,
    "IsSpeakingFlags": 0,
