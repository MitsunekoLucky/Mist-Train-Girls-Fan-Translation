        "CameraRotation": null
    }
},
{
    "GroupOrder": 12,
    "ViewOrder": 1,
    "Name": "カールスルーエ",
    "Phrase": "「むしろ、あなた何でここにいるんです？\\n　あなたのご両親が故郷のお屋敷にて、\\n　世界中から集めたお見合い相手を揃えて待っているはずですけど？」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": 19201,
    "IsSpeakingFlags": 0,
    "FrameImage": 0,
    "VoiceFileName": "m_300604_006",
