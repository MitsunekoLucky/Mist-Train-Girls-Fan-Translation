{
    "GroupOrder": 8,
    "ViewOrder": 1,
    "Name": "コロンビア",
    "Phrase": "「ええ。でも当然、貴方たちのような人材も組織には必要よ」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": null,
