},
{
    "GroupOrder": 10,
    "ViewOrder": 1,
    "Name": "",
    "Phrase": "物語の展開上、あさましいおチンポを挿入することにした。",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
