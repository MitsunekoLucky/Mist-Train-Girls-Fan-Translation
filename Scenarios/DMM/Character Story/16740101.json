},
{
    "GroupOrder": 10,
    "ViewOrder": 1,
    "Name": "コロンビア",
    "Phrase": "「数日後のバレンタインを楽しみにしてる他の女の子たちをよそに、\\n　いちばん最初に車掌くんにチョコを渡して……。\\n　かわいく愛の告白をしたりして、いちゃいちゃするはずだったのに」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": null,
    "IsSpeakingFlags": 4,
    "FrameImage": 0,
