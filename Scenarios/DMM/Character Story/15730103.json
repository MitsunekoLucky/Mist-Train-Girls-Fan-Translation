},
{
    "GroupOrder": 18,
    "ViewOrder": 1,
    "Name": "ブランシュ",
    "Phrase": "「なぜか私、そういうひとを惹きつけちゃうみたいなんだよね。\\n　自分にとって都合の良い、求めている女だって――\\n　そういう趣味の男のひとには、本能的にわかっちゃうのかな」",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": null,
    "IsSpeakingFlags": 4,
    "FrameImage": 0,
    "VoiceFileName": "c_15730103_008",
