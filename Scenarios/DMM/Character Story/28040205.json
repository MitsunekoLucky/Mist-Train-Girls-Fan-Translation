                "CameraZoomRatio": null,
                "CameraPositionX": null,
                "CameraPositionY": null,
                "CameraWorkDuration": null,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 16,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「恥ずかしいと思わないの？\\n　遅刻をした挙句、こんな格好で弄ばれて……情けなくならない？」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
            "VoiceFileName": "s_28040205_009",
