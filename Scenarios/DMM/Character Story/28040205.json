                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 27,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「こんなにお汁も溢れさせて……よっぽど私の足技が効いているのね。\\n　ほら、こっちの筋はどう？　それとも、先端部分を可愛がられたい？」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
            "VoiceFileName": "s_28040205_015",
            "BackgroundImage": "",
