                "AnimationSpeed": null,
                "CameraZoomRatio": 1.2,
                "CameraPositionX": 50,
                "CameraPositionY": 0,
                "CameraWorkDuration": 2,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 21,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「何をぼんやりとしているの？\\n　その顔……どう見ても、反省しているようには見えないけど」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
            "VoiceFileName": "s_28040205_012",
            "BackgroundImage": "",
