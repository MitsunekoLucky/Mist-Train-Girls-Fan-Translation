                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 29,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「あら、車掌さんのここがすごく震えているわね……？\\n　もしかして、限界が近いのかしら」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
