                "AnimationSpeed": null,
                "CameraZoomRatio": null,
                "CameraPositionX": null,
                "CameraPositionY": null,
                "CameraWorkDuration": null,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 20,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「ここもすごく熱くて、息も上がってきて、だいぶ苦しそうね？\\n　でも、これだけでは終わらないわよ」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
            "VoiceFileName": "s_28040205_011",
            "BackgroundImage": "",
            "BGM": "",
            "SE": "",
            "Effect": null,
            "BackgroundEffectType": 0,
