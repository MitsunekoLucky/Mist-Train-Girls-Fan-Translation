        },
        {
            "GroupOrder": 14,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「こんなにはちきれんばかりに膨らませておいて、よく言えるわね。\\n　これじゃあ、ご褒美になってしまうかしら？」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
