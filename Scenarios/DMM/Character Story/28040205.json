            }
        },
        {
            "GroupOrder": 44,
            "ViewOrder": 1,
            "Name": "",
            "Phrase": "微笑むブラックフライアの瞳に、思わず生唾を飲み込む。",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
