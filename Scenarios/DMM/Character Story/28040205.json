                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 25,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「だめよ車掌さん、そんなに気持ちよさそうな顔をしたら……。\\n　これはお仕置きなのよ？　辱められている自覚はある？」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
