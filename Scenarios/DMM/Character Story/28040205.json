                "CameraWorkDuration": null,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 22,
            "ViewOrder": 1,
            "Name": "",
            "Phrase": "この状況で反省に意識を傾ける方に無理がある。\\nブラックフライアは、わかっていながらそう聞いているようだ。",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": null,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
            "VoiceFileName": "",
            "BackgroundImage": "",
            "BGM": "",
            "SE": "",
            "Effect": null,
            "BackgroundEffectType": 0,
