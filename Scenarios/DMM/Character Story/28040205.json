            }
        },
        {
            "GroupOrder": 45,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「どうなるか、わかっているわね？」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
