                "CameraPositionY": null,
                "CameraWorkDuration": null,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 23,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「ここをなぞると……ふふ、面白いくらいに反応するわね。\\n　ここが車掌さんのいいところなの？」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
            "VoiceFileName": "s_28040205_013",
            "BackgroundImage": "",
            "BGM": "",
            "SE": "",
            "Effect": null,
