                "CameraPositionX": null,
                "CameraPositionY": null,
                "CameraWorkDuration": null,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 11,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「まあ、訓練と言ったけど実際にはお仕置きね。\\n　車掌さんの自分の立場に対する自覚と、反省を促すための辱めよ」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
            "VoiceFileName": "s_28040205_006",
            "BackgroundImage": "",
            "BGM": "",
            "SE": "",
            "Effect": null,
            "BackgroundEffectType": 0,
