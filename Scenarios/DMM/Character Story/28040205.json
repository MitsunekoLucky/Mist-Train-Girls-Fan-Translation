            }
        },
        {
            "GroupOrder": 46,
            "ViewOrder": 1,
            "Name": "",
            "Phrase": "わかるような、わからないような……しかし、言及は避けた方がいいだろう。\\nブラック教官の強烈な訓練で、こちらはもう限界なのだから――",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": null,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
