                "AnimationSpeed": null,
                "CameraZoomRatio": null,
                "CameraPositionX": null,
                "CameraPositionY": null,
                "CameraWorkDuration": null,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 18,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「車掌さんなんて、私にかかれば足だけでも封じ込めることが可能だと、\\n　これで証明できてしまったわね」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
            "VoiceFileName": "s_28040205_010",
            "BackgroundImage": "",
            "BGM": "",
            "SE": "",
            "Effect": null,
            "BackgroundEffectType": 0,
