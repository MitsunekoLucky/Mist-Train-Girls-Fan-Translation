                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 26,
            "ViewOrder": 1,
            "Name": "デトロイト",
            "Phrase": "「ボッタクリの店とか、\\n　いかがわしい薬を売る奴が増えたってことか」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": null,
            "IsSpeakingFlags": 4,
            "FrameImage": 0,
