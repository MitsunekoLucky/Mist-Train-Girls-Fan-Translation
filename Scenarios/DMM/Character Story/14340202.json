                "CameraWorkDuration": null,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 41,
            "ViewOrder": 1,
            "Name": "",
            "Phrase": "そして、そのスカジャンはあんたに持っていってほしい、\\nと告げたのだった。",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": null,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
