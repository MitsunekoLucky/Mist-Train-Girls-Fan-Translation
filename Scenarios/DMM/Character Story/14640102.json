        "AnimationSpeed": null,
        "CameraZoomRatio": null,
        "CameraPositionX": null,
        "CameraPositionY": null,
        "CameraWorkDuration": null,
        "CameraRotation": null
    }
},
{
    "GroupOrder": 19,
    "ViewOrder": 1,
    "Name": "ネピドー",
    "Phrase": "『やっぱり理屈は不明だけど、ほんとに無限に落ちたんだよ。\\n　膨大な魔力が渦巻いてるせいで時間や空間が変になってたのかなぁ、\\n　何百年も『魔の渦』のなかにいた気さえするの』",
    "PhraseFontSize": 2,
    "PhraseSpeed": 3,
    "IsPhraseCenter": false,
    "SpeechMCharacterId": null,
    "IsSpeakingFlags": 4,
    "FrameImage": 0,
    "VoiceFileName": "c_14640102_014",
    "BackgroundImage": "",
