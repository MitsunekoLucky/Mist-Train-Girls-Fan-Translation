        },
        {
            "GroupOrder": 4,
            "ViewOrder": 1,
            "Name": "",
            "Phrase": "ブラックフライアの背中が、腰が、お尻が――\\n全てが、誘っているように見えてしまう。",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": null,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
