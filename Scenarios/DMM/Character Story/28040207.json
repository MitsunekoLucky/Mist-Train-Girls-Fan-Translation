        {
            "GroupOrder": 48,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「普通に……誘ってくれたらよかったのに……」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
