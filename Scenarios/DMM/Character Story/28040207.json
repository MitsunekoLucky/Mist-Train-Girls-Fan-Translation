        {
            "GroupOrder": 46,
            "ViewOrder": 1,
            "Name": "ブラックフライア",
            "Phrase": "「本当にもう、仕方のない人……」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 80402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
