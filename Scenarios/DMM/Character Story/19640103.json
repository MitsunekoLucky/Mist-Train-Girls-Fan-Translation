        },
        {
            "GroupOrder": 31,
            "ViewOrder": 1,
            "Name": "シーイェン",
            "Phrase": "「もう簡単に漫画のシーンを試したい、\\n　なんて言わないようにする」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": null,
            "IsSpeakingFlags": 4,
            "FrameImage": 0,
