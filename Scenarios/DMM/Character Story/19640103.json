                "AnimationSpeed": null,
                "CameraZoomRatio": null,
                "CameraPositionX": null,
                "CameraPositionY": null,
                "CameraWorkDuration": null,
                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 30,
            "ViewOrder": 1,
            "Name": "シーイェン",
            "Phrase": "「ありがとう車掌。\\n　あたし、まだまだ未熟だった。\\n　圧倒的に修行が足りてなかったよ」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": null,
            "IsSpeakingFlags": 4,
            "FrameImage": 0,
            "VoiceFileName": "c_19640103_016",
