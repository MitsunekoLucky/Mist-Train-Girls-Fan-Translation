        },
        {
            "GroupOrder": 38,
            "ViewOrder": 1,
            "Name": "",
            "Phrase": "まあ、シーイェンの成長を手助け出来たと思えば、いいか……。",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": null,
