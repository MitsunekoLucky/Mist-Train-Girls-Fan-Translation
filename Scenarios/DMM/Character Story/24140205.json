                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 46,
            "ViewOrder": 1,
            "Name": "パッスィ",
            "Phrase": "「うふふ、いっぱい喜んでくださったんですね。\\n　すごく嬉しいです！」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 41402,
