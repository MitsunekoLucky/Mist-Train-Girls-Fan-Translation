            "GroupOrder": 18,
            "ViewOrder": 1,
            "Name": "パッスィ",
            "Phrase": "「……こんな感じでよろしいでしょうか？」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 41402,
            "IsSpeakingFlags": 0,
