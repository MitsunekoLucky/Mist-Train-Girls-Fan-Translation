                "CameraRotation": null
            }
        },
        {
            "GroupOrder": 47,
            "ViewOrder": 1,
            "Name": "パッスィ",
            "Phrase": "「でも車掌さま、次は一人だけで気持ちよくならずに……\\n　私のことも気持ちよくしてくださいね♪」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 41402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
