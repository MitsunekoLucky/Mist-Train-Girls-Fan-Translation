        },
        {
            "GroupOrder": 16,
            "ViewOrder": 1,
            "Name": "パッスィ",
            "Phrase": "「こんなところを見たら、子ブタちゃんと呼んでいいのかも\\n　迷い始めてしまいますね……」",
            "PhraseFontSize": 2,
            "PhraseSpeed": 3,
            "IsPhraseCenter": false,
            "SpeechMCharacterId": 41402,
            "IsSpeakingFlags": 0,
            "FrameImage": 0,
