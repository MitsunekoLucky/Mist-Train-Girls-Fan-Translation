            },
            {
                "GroupOrder": 19,
                "ViewOrder": 1,
                "Name": "",
                "Phrase": "It's the reason why she's as innocent and pure as a child in the best sense of the word, and pretty much an idiot in the worst. This is not my opinion, but that's what I read in a book about her upbringing as a celebrity.",
                "PhraseFontSize": 2,
                "PhraseSpeed": 1,
                "IsPhraseCenter": false,
                "SpeechMCharacterId": null,
                "IsSpeakingFlags": 0,
                "FrameImage": 0,
